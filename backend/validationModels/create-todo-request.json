{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "group",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1
      },
      "dueDate": {
        "type": "string",
        "minLength": 10,
        "maxLength": 10
      },
      "done": {
        "type": "boolean"
      },
      "attachmentUrl": {
        "type": "string"
      }
    },
    "required": [
      "name",
      "dueDate"
    ],
    "additionalProperties": false
  }